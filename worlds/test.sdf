<sdf version='1.9'>
  <world name='test'>

    <plugin name='ignition::gazebo::systems::Physics' filename='ignition-gazebo-physics-system'/>
    <plugin name='ignition::gazebo::systems::UserCommands' filename='ignition-gazebo-user-commands-system'/>
    <plugin name='ignition::gazebo::systems::SceneBroadcaster' filename='ignition-gazebo-scene-broadcaster-system'/>
    <plugin name='ignition::gazebo::systems::Contact' filename='ignition-gazebo-contact-system'/>
    <plugin name='ignition::gazebo::systems::ParticleEmitter2' filename='ignition-gazebo-particle-emitter2-system'/>

    <!-- From rs1_robot simple_trees_builtin.sdf plugin fix -->

    <plugin name='ignition::gazebo::systems::Sensors' filename='ignition-gazebo-sensors-system'>
      <render_engine>ogre2</render_engine>
    </plugin>

    <plugin name='ignition::gazebo::systems::Imu' filename='ignition-gazebo-imu-system' />
    <plugin name='ignition::gazebo::systems::NavSat' filename='ignition-gazebo-navsat-system' />

    <gravity>0 0 -9.81</gravity>
    <magnetic_field>6e-06 2.3e-05 -4.2e-05</magnetic_field>
    <atmosphere type='adiabatic'/>
    <physics name='default_physics' type='ignored'>
      <max_step_size>0.01</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>100</real_time_update_rate>
    </physics>
    <scene>
      <ambient>1 1 1 1</ambient>
      <background>0.6 0.8 1 1</background>
      <shadows>false</shadows>
      <grid>false</grid>
    </scene>
    <spherical_coordinates>
      <latitude_deg>0</latitude_deg>
      <longitude_deg>0</longitude_deg>
      <elevation>10</elevation>
      <heading_deg>0</heading_deg>
      <surface_model>EARTH_WGS84</surface_model>
    </spherical_coordinates>

    <!-- Mountain Forest Terrain-->
    <include>
      <uri>model://mountain_forest</uri>
      <name>mountain_forest</name>
      <pose>0 0 0 0 -0 0</pose>
    </include>

    <!-- Person Model List -->
    <include>
	      <uri>
		      model://person
	      </uri>
	      <name>person1</name>
	      <pose>2 10 1 0 0 0</pose>
	</include>

    <include>
        <uri>
            model://lowpolytree_1
        </uri>
        <name>lowpolytree_1</name>
        <pose>5 5 0 0 0 0</pose>
    </include>

    <include>
        <uri>
            model://lowpolytree_2
        </uri>
        <name>lowpolytree_2</name>
        <pose>5 10 0 0 0 0</pose>
    </include>

    <include>
        <uri>
            model://lowpolytree_3
        </uri>
        <name>lowpolytree_3</name>
        <pose>5 15 0 0 0 0</pose>
    </include>

    <include>
        <uri>
            model://lowpolytree_4
        </uri>
        <name>lowpolytree_4</name>
        <pose>5 -5 0 0 0 0</pose>
    </include>

    <include>
        <uri>
            model://lowpolytree_5
        </uri>
        <name>lowpolytree_5</name>
        <pose>5 -10 0 0 0 0</pose>
    </include>

    <include>
        <uri>
            model://fence_1
        </uri>
        <name>fence_1</name>
        <pose>0 0 0 0 0 0</pose>
    </include>

    <include>
        <uri>
            model://fence_2
        </uri>
        <name>fence_2</name>
        <pose>2 2 0 0 0 0</pose>
    </include>

    <include>
        <uri>
            model://boulder_1
        </uri>
        <name>boulder_1</name>
        <pose>-5 5 0 0 0 0</pose>
    </include>

    <include>
        <uri>
            model://boulder_2
        </uri>
        <name>boulder_2</name>
        <pose>-10 5 0 0 0 0</pose>
    </include>

    <light name='sun' type='directional'>
      <pose>0 0 100 0 -0 0</pose>
      <cast_shadows>false</cast_shadows>
      <intensity>5</intensity>
      <direction>-0.5 0.1 -0.9</direction>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.8 0.8 0.8 1</specular>
      <attenuation>
        <range>10</range>
        <linear>1</linear>
        <constant>1</constant>
        <quadratic>0</quadratic>
      </attenuation>
      <spot>
        <inner_angle>0</inner_angle>
        <outer_angle>0</outer_angle>
        <falloff>0</falloff>
      </spot>
    </light>

  </world>
</sdf>
